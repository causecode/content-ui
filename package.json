{
  "name": "content-ui",
  "version": "1.0.0",
  "description": "React version of static-content",
  "main": "webpack.config.js",
  "scripts": {
    "tar": "npm run build:prod && tar -czvf www.tar.gz www/",
    "start": "npm run clean && webpack-dev-server --production",
    "build": "npm run clean && webpack",
    "build:dev": "npm run build",
    "build:prod": "npm run build -- --production",
    "clean": "rimraf public www",
    "compile": "npm run clean && tsc",
    "postInstall": "npm run compile",
    "RH-generate-edit": "RH-generate-edit",
    "RH-generate-show": "RH-generate-show",
    "test": "NODE_ENV=test jest --no-cache",
    "lint-ts": "tslint 'src/*.tsx' 'src/**/*.tsx' 'src/*.ts' 'src/**/*.ts'",
    "start:dev": "npm run clean && webpack-dev-server --watch --hot"
  },
  "reactHero": {
    "serverUrl": "http://localhost:8080",
    "APIUrl": "http://localhost:8080/api/v1/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/causecode/content-ui.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/causecode/content-ui#readme",
  "dependencies": {
    "radium": "^0.18.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.2.1",
    "react-fontawesome": "^1.3.1",
    "react-hero": "git+ssh://git@bitbucket.org/causecode/react-hero.git#functional-fixes",
    "react-redux-form": "^1.2.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tinymce": "^0.5.1"
  }
}
